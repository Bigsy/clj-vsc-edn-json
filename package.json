{
  "name": "clj-vsc-formatter",
  "displayName": "ClojureScript JSON to EDN",
  "description": "VSCode extension to convert JSON to Clojure EDN",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [],
  "main": "./out/main.js",
  "contributes": {
    "commands": [{
      "command": "clj-vsc-formatter.jsonToEdn",
      "title": "Convert JSON to EDN"
    }],
    "menus": {
      "editor/context": [{
        "command": "clj-vsc-formatter.jsonToEdn",
        "when": "editorHasSelection",
        "group": "1_modification"
      }]
    }
  },
  "scripts": {
    "watch": "shadow-cljs watch extension",
    "compile": "shadow-cljs compile extension",
    "release": "shadow-cljs release extension"
  },
  "devDependencies": {
    "shadow-cljs": "^2.25.2"
  }
}